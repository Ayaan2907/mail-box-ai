// This is your Prisma schema file,
// learn more about it in the docs: https://pris.ly/d/prisma-schema

generator client {
    provider = "prisma-client-js"
}

datasource db {
    provider  = "postgresql"
    url       = env("DATABASE_URL")
    directUrl = env("DATABASE_URL_UNPOOLED")
}

model User {
    id          String
    name        String
    email       String   @unique
    image_url     String?
    recorded_at DateTime  //recorded in our database
    created_at  String
    updated_at  String

    @@index([name])
}
